{
  "fileTypes": ["js", "mjs", "jsx", "ts", "mts", "tsx"],
  "injectionSelector": "L:source.js -comment -(string -meta.embedded), L:source.mjs -comment -(string -meta.embedded), L:source.jsx -comment -(string -meta.embedded), L:source.ts -comment -(string -meta.embedded), L:source.mts -comment -(string -meta.embedded), L:source.tsx -comment -(string -meta.embedded)",
  "injections": {
    "L:source": {
      "patterns": [
        {
          "match": "<",
          "name": "invalid.illegal.bad-angle-bracket.html"
        }
      ]
    }
  },
  "patterns": [
    {
      "contentName": "meta.embedded.block.sld",
      "begin": "(?i)(\\b\\w*(?:sld)\\w*)(`)",
      "beginCaptures": {
        "1": {
          "name": "entity.name.function.sld"
        },
        "2": {
          "name": "punctuation.definition.string.template.begin.js"
        }
      },
      "end": "(`)",
      "endCaptures": {

        "1": {
          "name": "punctuation.definition.string.template.end.js"
        }
      },
      "patterns": [
        {
          "include": "source.ts#template-substitution-element"
        },
        {
          "include": "source.sld"
        }
      ]
    }
  ],
  "scopeName": "inline.sld"
}
